<div class="container-fluid mt-0">
  <!-- Titre "competence" -->
  <div class="row mb-3 mt-0">
    <div class="col text-center">
      <h1 class="display-4 header-title" id="top">Mes compétences</h1>
      <p class="lead text-muted">Découvrez les compétences techniques et professionnelles que j'ai développées au cours de ma formation.</p>
    </div>
  </div>

  <!-- Cartes de compétences par catégories -->
<div class="row justify-content-center mb-5">
  <% @categories.each do |category| %>
    <div class="col-md-5 m-3 p-4 border rounded shadow-sm bg-light category-card">
      <h2 class="text-center fw-bold mb-3"><%= category.title %></h2>
      
      <div class="row gx-3 gy-3">
        <% category.competences.each do |competence| %>
          <% if competence.projets.any? %>
            <div class="col-md-4 upEffect mb-3">
              <% if competence.projets.any? %>
                <%= link_to projets_path(filter: {competence_ids: [competence.id]}), class: "text-decoration-none" do %>
                  <div class="p-3 border rounded text-center shadow-sm bg-white" style="width: 100%; aspect-ratio: 1/1;">
                    <div class="d-flex justify-content-center align-items-center" style="height: 70%;">
                      <% if competence.logo.present? %>
                        <% if competence.logo.include?('.') %>
                          <%= image_tag competence.logo, alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                        <% else %>
                          <%= image_tag url_for(competence.logo), alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                        <% end %>
                      <% else %>
                        <i class="bi bi-check-circle-fill text-success fs-1"></i>
                      <% end %>
                    </div>
                    <% if competence.useTitle == true %>
                      <h3 class="h6 fw-semibold mt-2"><%= competence.title %></h3>
                    <% end %>
                  </div>
                <% end %>
              <% else %>
                <div class="p-3 border rounded text-center shadow-sm bg-white" style="width: 100%; aspect-ratio: 1/1;">
                  <div class="d-flex justify-content-center align-items-center" style="height: 70%;">
                    <% if competence.logo.present? %>
                      <% if competence.logo.include?('.') %>
                        <%= image_tag competence.logo, alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                      <% else %>
                        <%= image_tag url_for(competence.logo), alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                      <% end %>
                    <% else %>
                      <i class="bi bi-check-circle-fill text-success fs-1"></i>
                    <% end %>
                  </div>
                  <% if competence.useTitle == true %>
                    <h3 class="h6 fw-semibold mt-2"><%= competence.title %></h3>
                  <% end %>
                </div>
              <% end %>
              <% if session[:admin] %>
                <div class="mt-2 text-center">
                  <%= link_to "Editer", edit_competence_path(competence.id), class: "btn btn-success btn-sm py-0 px-2" %>
                  <%= link_to "Supprimer", competence_path(competence.id), 
                      method: :delete,
                      data: { confirm: "Êtes-vous sûr ?" },
                      class: "btn btn-danger btn-sm py-0 px-2" %>
                </div>
              <% end %>
            </div>
          <% else %>
            <div class="col-md-4 mb-3">
              <% if competence.projets.any? %>
                <%= link_to projets_path(filter: {competence_ids: [competence.id]}), class: "text-decoration-none" do %>
                  <div class="p-3 border rounded text-center shadow-sm bg-white" style="width: 100%; aspect-ratio: 1/1;">
                    <div class="d-flex justify-content-center align-items-center" style="height: 70%;">
                      <% if competence.logo.present? %>
                        <% if competence.logo.include?('.') %>
                          <%= image_tag competence.logo, alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                        <% else %>
                          <%= image_tag url_for(competence.logo), alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                        <% end %>
                      <% else %>
                        <i class="bi bi-check-circle-fill text-success fs-1"></i>
                      <% end %>
                    </div>
                    <% if competence.useTitle == true %>
                      <h3 class="h6 fw-semibold mt-2"><%= competence.title %></h3>
                    <% end %>
                  </div>
                <% end %>
              <% else %>
                <div class="p-3 border rounded text-center shadow-sm bg-white" style="width: 100%; aspect-ratio: 1/1;">
                  <div class="d-flex justify-content-center align-items-center" style="height: 70%;">
                    <% if competence.logo.present? %>
                      <% if competence.logo.include?('.') %>
                        <%= image_tag competence.logo, alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                      <% else %>
                        <%= image_tag url_for(competence.logo), alt: competence.title, class: "img-fluid", style: "max-height: 100%; max-width: 100%; object-fit: contain;" %>
                      <% end %>
                    <% else %>
                      <i class="bi bi-check-circle-fill text-success fs-1"></i>
                    <% end %>
                  </div>
                  <% if competence.useTitle == true %>
                    <h3 class="h6 fw-semibold mt-2"><%= competence.title %></h3>
                  <% end %>
                </div>
              <% end %>
              <% if session[:admin] %>
                <div class="mt-2 text-center">
                  <%= link_to "Editer", edit_competence_path(competence.id), class: "btn btn-success btn-sm py-0 px-2" %>
                  <%= link_to "Supprimer", competence_path(competence.id), 
                      method: :delete,
                      data: { confirm: "Êtes-vous sûr ?" },
                      class: "btn btn-danger btn-sm py-0 px-2" %>
                </div>
              <% end %>
            </div>
        <% end %>
        <% end %>
      </div>
    </div>
  <% end %>
</div>

  <% if session[:admin] %>
    <div class="text-center mt-4">
      <a href="<%= new_competence_path %>" class="btn btn-success">Ajouter une nouvelle compétence</a>
    </div>
  <% end %>
  
